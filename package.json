{
  "name": "rsvp",
  "version": "0.1.0",
  "private": true,
  "description": "DeVaux's RSVP app",
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate & next build",
    "typecheck": "next build",
    "start": "next start",
    "lint": "next lint",
    "migrate": "dotenv -e .env.local -- prisma migrate dev",
    "generate": "dotenv -e .env.local -- prisma generate",
    "env": "pnpx vercel env pull",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky",
    "sync_translation": "dotenv -e .env.local -- ts-node ./scripts/sync_keys.ts",
    "look_keys": "dotenv -e .env.local -- ts-node ./scripts/look_keys.ts",
    "translate": "pnpm run look_keys && pnpm run sync_translation"
  },
  "dependencies": {
    "@clerk/localizations": "^2.7.1",
    "@clerk/nextjs": "^5.3.7",
    "@prisma/client": "^5.19.0",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-menubar": "^1.0.4",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@tanstack/react-query": "^5.53.1",
    "@types/nprogress": "^0.2.3",
    "@vercel/blob": "^0.22.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "formik": "^2.4.5",
    "framer-motion": "^7.10.3",
    "lucide-react": "^0.309.0",
    "next": "14.0.4",
    "next-themes": "^0.2.1",
    "nextjs-toploader": "^1.6.4",
    "nprogress": "^0.2.0",
    "prisma": "^5.19.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "remove-accents": "^0.4.4",
    "sharp": "^0.33.3",
    "sonner": "^1.3.1",
    "svix": "^1.15.0",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@clerk/types": "^4.17.0",
    "@tanstack/eslint-plugin-query": "^5.53.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@types/jest": "^29.5.12",
    "@types/node": "20.11.2",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "^10.4.16",
    "dotenv": "^16.4.5",
    "eslint": "8.57.0",
    "eslint-config-frbosquet-tsnext": "^1.0.2",
    "husky": "^9.1.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "openai": "^4.58.1",
    "plugin": "link:@types/tailwindcss/plugin",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "5.3.3"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}